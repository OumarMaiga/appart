<p id="notice"><%= notice %></p>

<h1>Caracteristiques</h1>

<table>
  <thead>
    <tr>
      <th>Libelle</th>
      <th>Icon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @caracteristiques.each do |caracteristique| %>
      <tr>
        <td><%= caracteristique.libelle %></td>
        <td><%= caracteristique.icon %></td>
        <td><%= link_to 'Show', caracteristique %></td>
        <td><%= link_to 'Edit', edit_caracteristique_path(caracteristique) %></td>
        <td><%= link_to 'Destroy', caracteristique, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caracteristique', new_caracteristique_path %>
